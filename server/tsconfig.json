{
  "compilerOptions": {
    "target": "es2022",
    "module": "CommonJS",
    "baseUrl": "./",
    "outDir": "./out",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "@fastify/express",
    ],
    "paths": {
      "shared/*": [
        "../shared/*"
      ]
    }
  },
}